config["echo"]="echo 'HELLO'"
config["stress"]="touch stress.lock; stress -c 2 -t 59; rm stress.lock"
config["stress1"]="touch stress1.lock; stress -c 1 -t 59; rm stress1.lock"
config["stress4"]="touch stress4.lock; stress -c 4 -t 59; rm stress4.lock"
config["stress8"]="touch stress8.lock; stress -c 8 -t 59; rm stress8.lock"
config["stress_r2"]="touch stress_r2.lock; stress -c $(($RANDOM % 2 + 1)) -t 59; rm stress_r2.lock"
config["stress_r4"]="touch stress_r4.lock; stress -c $(($RANDOM % 4 + 1)) -t 59; rm stress_r4.lock"
